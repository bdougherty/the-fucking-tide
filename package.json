{
  "name": "the-fucking-tide",
  "description": "Check the fucking tide.",
  "version": "1.0.0",
  "author": "Brad Dougherty <me@brad.is>",
  "license": "MIT",
  "repository": "bdougherty/the-fucking-tide",
  "private": true,
  "scripts": {
    "dev": "node --inspect=0 node_modules/.bin/sapper dev -p 3103",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "lint": "xo",
    "test": "npm-run-all lint --parallel --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "polka": "^0.5.2",
    "sirv": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "chokidar": "^2.1.2",
    "eslint-config-bdougherty": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.4.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.25.0",
    "svelte": "^2.16.1",
    "xo": "^0.24.0"
  },
  "xo": {
    "extends": "bdougherty/sapper",
    "extensions": [
      "html"
    ],
    "ignores": [
      "cypress/**"
    ]
  }
}
